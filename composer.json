{
    "config": {
        "autoloader-suffix": "IJENKO",
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "Itscaro\\": "src/"
        }
    },
    "bin": ["bin/run.php"],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/itscaro/phpflickr"
        },
        {
            "type": "vcs",
            "url": "https://github.com/itscaro/zf2-restful"
        },
        {
            "type": "vcs",
            "url": "https://github.com/zendframework/zendoauth"
        }
    ],
    "require": {
        "php": ">=5.3.11",
        "ext-curl": "*",
        "lib-curl": "*",
        "lib-libxml": "*",
        "lib-openssl": "*",
        "lib-pcre": "*",
        "Symfony/Console": "*",
        "Symfony/Finder": "*",
        "Symfony/Filesystem": "*",
        "Symfony/Process": "*",
        "Symfony/Yaml": "*",
        "Symfony/Config": "*",
        "monolog/monolog": "*",
        "netresearch/jsonmapper": "*",
        "itscaro/zf2-restful": "dev-master"
    },
    "require-dev": {
    }
}
